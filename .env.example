# 心迹 (XinJi) 环境变量配置模板
# 复制此文件为 .env 并填入实际值

# MySQL
MYSQL_ROOT_PASSWORD=your_root_password
MYSQL_PASSWORD=your_xinji_password

# Redis
REDIS_PASSWORD=your_redis_password

# JWT 密钥 (至少32位)
JWT_SECRET=your-256-bit-secret-key-here-change-it-to-random

# AES 加密密钥 (16位)
AES_SECRET=your-aes-key-16b

# 阿里云 DashScope API Key (用于AI情绪分析)
DASHSCOPE_API_KEY=sk-your-dashscope-api-key

# 微信支付配置 (可选)
WECHAT_PAY_MERCHANT_ID=your_merchant_id
WECHAT_PAY_APP_ID=your_app_id
WECHAT_PAY_API_KEY=your_api_key
WECHAT_PAY_SERIAL_NO=your_certificate_serial_no
